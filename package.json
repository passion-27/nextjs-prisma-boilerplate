  "repository": {
    "type": "git",
    "url": "https://github.com/nemanjam/nextjs-prisma-boilerplate"
  },
  "homepage": "https://github.com/nemanjam/nextjs-prisma-boilerplate",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/index.js",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn lint && yarn type-check && yarn format",
    "test": "jest",
    "test-ci": "jest --coverage --silent --ci",
    "studio": "dotenv -e .env.local -- sh -c 'npx prisma studio'",
    "migrate-dev": "npx prisma migrate dev --skip-seed",
    "migrate-dev-env": "dotenv -e .env.local -- sh -c 'npx prisma migrate dev --skip-seed'",
    "reset-dev": "dotenv -e .env.local -- sh -c 'npx prisma migrate reset dev --skip-seed'",
    "start-dev": "yarn migrate-dev && yarn seed && yarn dev",
    "migrate-prod": "npx prisma migrate deploy",
    "start-prod": "yarn migrate-prod && yarn seed && yarn start",
    "seed": "npx prisma db seed",
    "seed-env": "dotenv -e .env.local -- sh -c 'npx prisma db seed'",
    "docker-build-dev": "docker build -f Dockerfile.dev -t nextjs-prisma-dev .",
    "dc-build-dev": "docker-compose -f docker-compose.dev.yml nextjs-prisma-dev build",
    "dc-dev": "docker-compose -f docker-compose.dev.yml up",
    "docker-build-prod": "dotenv -e .env.local -- sh -c 'docker build -f Dockerfile.prod -t nextjs-prisma-prod --build-arg ARG_DATABASE_URL=${DATABASE_URL} .'",
    "dc-build-prod": "docker-compose -f docker-compose.prod.yml nextjs-prisma-prod --env-file=.env.local build",
    "dc-prod": "docker-compose -f docker-compose.prod.yml up",
    "test-script": "dotenv -e .env.local -- sh -c 'echo $DATABASE_URL'"
  },
  "dependencies": {
    "@faker-js/faker": "^6.1.1",
    "@hookform/resolvers": "^2.8.8",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.11.1",
    "axios": "^0.26.1",
    "b_": "^1.3.4",
    "babel-plugin-superjson-next": "^0.4.3",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "next": "^12.1.2",
