{
  "name": "nextjs-prisma-boilerplate",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/index.js",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn lint && yarn type-check && yarn format",
    "studio": "dotenv -e .env.local -- sh -c 'npx prisma studio'",
    "migrate-dev": "npx prisma migrate dev --skip-seed",
    "migrate-dev-env": "dotenv -e .env.local -- sh -c 'npx prisma migrate dev --skip-seed'",
    "reset-dev": "dotenv -e .env.local -- sh -c 'npx prisma migrate reset dev --skip-seed'",
    "start-dev": "yarn migrate-dev && yarn seed && yarn dev",
    "migrate-prod": "npx prisma migrate deploy",
    "start-prod": "yarn migrate-prod && yarn seed && yarn start",
    "seed": "npx prisma db seed",
    "seed-env": "dotenv -e .env.local -- sh -c 'npx prisma db seed'",
    "docker-build-dev": "docker build -f Dockerfile.dev -t nextjs-prisma-dev .",
    "dc-build-dev": "docker-compose -f docker-compose.dev.yml nextjs-prisma-dev build",
    "dc-dev": "docker-compose -f docker-compose.dev.yml up",
    "docker-build-prod": "dotenv -e .env.local -- sh -c 'docker build -f Dockerfile.prod -t nextjs-prisma-prod --build-arg ARG_DATABASE_URL=${DATABASE_URL} .'",
    "dc-build-prod": "docker-compose -f docker-compose.prod.yml nextjs-prisma-prod --env-file=.env.local build",
    "dc-prod": "docker-compose -f docker-compose.prod.yml up",
    "test-script": "dotenv -e .env.local -- sh -c 'echo $DATABASE_URL'"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.8.0",
    "axios": "^0.24.0",
    "b_": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.2",
    "faker": "5.5.3",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "next": "12.0.8",
    "next-auth": "4.1.0",
    "next-connect": "^0.11.0",
    "next-validations": "^0.1.11",
    "prisma": "^3.8.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.5.1",
    "react-hook-form": "^7.23.0",
    "react-icons": "^4.3.1",
    "sass": "^1.48.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@types/b_": "^1.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "16.11.12",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^3.0.8",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
